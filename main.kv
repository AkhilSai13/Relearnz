#:kivy 2.0.0

WindowManager:
	Welcome:
	Login:
    Register:
    Tdashboard:
    Sdashboard:
    Scourse:
    Sevent:
    Schat:
    Sannouncement:
    Sfeedback:




<Welcome>:
    name: "Welcome"

    MDBoxLayout:
        orientation :"vertical"
        MDBoxLayout:
            MDLabel:
                text:""
                size_hint: 0.2 ,1   
            Image:
                source: 'assets\welcome.png'
                keep_ratio: False
                allow_stretch: True
            MDLabel:
                text:""
                size_hint: 0.2 ,1   
        MDLabel:
            text:"Welcome"
            font_style: "H2"
            halign: "center"
            size_hint: 1, 0.5

        MDFillRoundFlatButton:
            text:"Let's  get  started"
            md_bg_color:"green"
            font_style: "Button"
            pos_hint: {"center_x":0.5, "center_y":1}
			on_release:
				app.root.current = "Login"
				root.manager.transition.direction = "left"

        MDLabel:
            text:""
            size_hint: 1, 0.3


<Login>:
    name: "Login"

    MDScreen:
        MDSpinner:
            id : load
            size_hint: None, None
            size: dp(46), dp(46)
            pos_hint: {'center_x': .9, 'center_y': .1}
            active: False

    MDBoxLayout:
        orientation :"vertical"
        size_hint : 1, 1

        MDBoxLayout:
            MDLabel:
                text:""
                size_hint: 0.2 ,1   
            Image:
                source: 'assets\login.png'
                keep_ratio: False
                allow_stretch: True
            MDLabel:
                text:""
                size_hint: 0.2 ,1   

        MDBoxLayout:
            size_hint : 1, 0.4
            MDBoxLayout:
                orientation :"vertical"

                MDLabel:
                    text: "Username"
                    padding_x: 85
                    font_style: "H6"

                MDTextField:
                    id: user
                    mode: "rectangle"
                    icon_right: "account"
                    pos_hint:{"center_x":0.5, "center_y":1}
                    size_hint_x: 0.7
                    required: True
                    helper_text_mode: "on_error"
                    width: 200
                    font_size: 18


            MDBoxLayout:
                orientation :"vertical"
                MDLabel:
                    text: ""

                MDFillRoundFlatButton:
                    id:"one"
                    text: "Login"
                    font_style: "Button"
                    font_size: 12
                    md_bg_color: "purple"
                    pos_hint:{"center_x":0.3, "center_y":1}
                    on_press: root.login()


        MDBoxLayout:
            size_hint : 1, 0.4
            MDBoxLayout:
                orientation :"vertical"

                MDLabel:
                    text: "Password"
                    padding_x: 85
                    font_style: "H6"

                MDTextField:
                    id: password
                    mode: "rectangle"
                    icon_right: "eye-off"
                    size_hint_x: 0.7
                    pos_hint:{"center_x":0.5, "center_y":1}
                    width: 200
                    required: True
                    helper_text_mode: "on_error"
                    font_size: 18
                    password: False

            MDBoxLayout:
                orientation :"vertical"

                MDLabel:
                    text: "Don't have an account?"
                    padding_x: 105
                    font_style: "H6"

                MDFillRoundFlatButton:
                    text: "Signup"
                    font_size: 12
                    font_style: "Button"
                    md_bg_color: 1, 0.2, 0, 1
                    pos_hint:{"center_x":0.3, "center_y":1}
                    on_release:
                        app.root.current = "Register"
                        root.manager.transition.direction = "left"

        MDLabel:
            size_hint : 1, 0.3
            text: ""

